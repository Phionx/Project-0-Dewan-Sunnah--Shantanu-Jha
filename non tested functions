void printAll(){
    int i;
    for (i=0; i<26; i++)
        print_list(table[i]);
}

song_node DeleteSong(song_node s){
    song_node *node=table[findletter(s->name[0])];
while (node){
    if (strcmp(node->next->name,s->name)==0){
        song_node *ans=node->next;
        node->next=node->next->next;
        }
    node=node->next;
ans->next=0; return ans;
